<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<ScrollPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hellofx.ListingController">
   <content>
      <AnchorPane prefHeight="720.0" prefWidth="1280.0">
         <children>
            <ImageView fx:id="carPic" fitHeight="218.0" fitWidth="364.0" layoutX="172.0" layoutY="89.0" pickOnBounds="true">
               <viewport>
                  <Rectangle2D />
               </viewport>
               <image>
                  <Image url="@../CarPics/mcqueen1.jpg" />
               </image>
            </ImageView>
            <Button fx:id="perviousCar" layoutX="142.0" layoutY="190.0" mnemonicParsing="false" onAction="#previousImage" text="&lt;">
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Button>
            <Button fx:id="nextCar" layoutX="536.0" layoutY="184.0" mnemonicParsing="false" onAction="#nextImage" text="&gt;">
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Button>
            <Label layoutX="529.0" layoutY="14.0" text="Jams Auto Sales">
               <font>
                  <Font name="Arial Black" size="24.0" />
               </font>
            </Label>
            <Label fx:id="condition" layoutX="172.0" layoutY="318.0" prefHeight="18.0" prefWidth="67.0" text="Condition">
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Label>
            <Label fx:id="yearMakeModel" layoutX="172.0" layoutY="347.0" text="Year Make Model">
               <font>
                  <Font name="Arial" size="22.0" />
               </font>
            </Label>
            <Label fx:id="mileage" layoutX="172.0" layoutY="405.0" text="Mileage">
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Label>
            <Label fx:id="price" layoutX="171.0" layoutY="479.0" text="Price">
               <font>
                  <Font name="Arial Bold" size="28.0" />
               </font>
            </Label>
            <Label fx:id="titleStatus" layoutX="354.0" layoutY="318.0" prefHeight="18.0" prefWidth="76.0" text="Title Status">
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Label>
            <Label fx:id="zip" layoutX="354.0" layoutY="404.0" text="Zip Code">
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Label>
            <Label fx:id="desc" alignment="TOP_LEFT" layoutX="171.0" layoutY="554.0" prefHeight="148.0" prefWidth="390.0" text="Description" />
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
