<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<ScrollPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hellofx.ListingController">
   <content>
      <AnchorPane prefHeight="720.0" prefWidth="1280.0">
         <children>
            <ImageView fx:id="carPic" fitHeight="347.0" fitWidth="558.0" layoutX="172.0" layoutY="89.0" pickOnBounds="true">
               <viewport>
                  <Rectangle2D />
               </viewport>
            </ImageView>
            <Button fx:id="previousCar" layoutX="142.0" layoutY="249.0" mnemonicParsing="false" onAction="#previousImage" text="&lt;">
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Button>
            <Button fx:id="nextCar" layoutX="730.0" layoutY="248.0" mnemonicParsing="false" onAction="#nextImage" text="&gt;">
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Button>
            <Label layoutX="529.0" layoutY="14.0" text="Jams Auto Sales">
               <font>
                  <Font name="Arial Black" size="24.0" />
               </font>
            </Label>
            <Label fx:id="condition" layoutX="171.0" layoutY="456.0" prefHeight="18.0" prefWidth="67.0" text="Condition">
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Label>
            <Label fx:id="yearMakeModel" layoutX="171.0" layoutY="489.0" text="Year Make Model">
               <font>
                  <Font name="Arial" size="22.0" />
               </font>
            </Label>
            <Label fx:id="mileage" layoutX="171.0" layoutY="526.0" text="Mileage">
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Label>
            <Label fx:id="price" layoutX="170.0" layoutY="581.0" text="Price">
               <font>
                  <Font name="Arial Bold" size="28.0" />
               </font>
            </Label>
            <Label fx:id="titleStatus" layoutX="360.0" layoutY="456.0" prefHeight="18.0" prefWidth="76.0" text="Title Status">
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Label>
            <Label fx:id="zip" layoutX="336.0" layoutY="526.0" text="Zip Code">
               <font>
                  <Font name="Arial" size="15.0" />
               </font>
            </Label>
            <Label fx:id="desc" alignment="TOP_LEFT" layoutX="172.0" layoutY="632.0" prefHeight="29.0" prefWidth="390.0" text="Description" />
            <Button fx:id="backButton" layoutX="14.0" layoutY="20.0" mnemonicParsing="false" onAction="#goBack" text="Go Back" textFill="#963e3e">
               <font>
                  <Font name="Arial" size="12.0" />
               </font>
            </Button>
         </children>
      </AnchorPane>
   </content>
</ScrollPane>
